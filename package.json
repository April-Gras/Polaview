    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "prisma": "^4.0.0",
    "sass": "^1.54.0",
