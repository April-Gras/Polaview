    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "click-outside-vue3": "^4.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "pinia": "^2.0.16",
    "redis": "^4.3.1",
    "string-similarity": "^4.0.4",
    "subtitle": "^4.2.1",
    "systeminformation": "^5.12.6",
    "threads": "^1.7.0",
    "uuid": "^8.3.2",
